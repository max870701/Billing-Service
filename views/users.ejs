<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users Page</title>
  </head>
  <body>
    <ul>
      <% data.forEach(user => { %>
      <li><a href="/users/<%= user.id %>"><%= user.id %><%= user.name %></a></li>
      <% }) %>
    </ul>
  </body>
</html>
